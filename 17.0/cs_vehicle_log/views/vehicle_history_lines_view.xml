<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_vehicle_history_lines_form" model="ir.ui.view">
            <field name="name">vehicle.history.lines.form</field>
            <field name="model">vehicle.history.lines</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="date"/>
                        <field name="od_start_read"/>
                    </group>
                </form>
            </field>
        </record>


        <record id="view_vehicle_history_lines_tree" model="ir.ui.view">
            <field name="name">vehicle.history.lines.tree</field>
            <field name="model">vehicle.history.lines</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="date"/>
                    <field name="od_start_read"/>
                    <field name="yearly_distance"/>
                </tree>
            </field>
        </record>


        <record id="action_vehicle_history_lines" model="ir.actions.act_window">
            <field name="name">Vehicle History Lines</field>
            <field name="res_model">vehicle.history.lines</field>
            <field name="view_mode">tree,form</field>
        </record>


<!--        <menuitem id="menu_vehicle_history_lines"-->
<!--                  name="Vehicle History Lines"-->
<!--                  parent="menu_cs_vehicle_log_configuration_root"-->
<!--                  action="action_vehicle_history_lines"/>-->
    </data>
</odoo>

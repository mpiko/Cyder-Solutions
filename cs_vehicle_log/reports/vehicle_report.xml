<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="cs_vehicle_log_report_details">
        <t t-call="web.html_container">
            <t t-call="web.external_layout">
                <t t-foreach="docs" t-as="o">
                    <div class="page">
                        <div class="oe_structure"/>
                        <h1>Report</h1>
                        <p>Report <b><span  t-field="o.name"/> </b></p>
                        <table class="table table-condensed">

                            <tr><td>
                                    Rego
                                </td><td>
                                    <span t-field="o.name"/>
                                </td></tr>
                             <tr><td>
                                    Make
                                </td><td>
                                    <span t-field="o.veh_make"/>
                                </td></tr>
                             <tr><td>
                                    Model
                                </td><td>
                                    <span t-field="o.veh_model"/>
                                </td></tr>
                             <tr><td>
                                    Engine Cap
                                </td><td>
                                    <span t-field="o.veh_capacity"/>
                                </td></tr>

                        </table>
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>
